<Application x:Class="IPConnectionData.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converter="clr-namespace:IPConnectionData.App.Converter"
             xmlns:local="clr-namespace:IPConnectionData.App"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <local:ViewModelLocator x:Key="mainViewModelLocator"></local:ViewModelLocator>
            <converter:MultiBindConverter x:Key="localMultiBindConverter"></converter:MultiBindConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
